{
  "name": "my-app",
  "main": "index.js",
  "version": "1.0.1",
  "scripts": {
    "start": "npx expo start",
    "start:tunnel": "npx expo start --tunnel --clear",
    "start:clear": "npx expo start --clear",
    "deploy": "echo 'Use deploy:dev, deploy:staging, or deploy:prod instead'",
    "deploy:dev": "EXPO_PUBLIC_APP_ENV=development npx expo export -p web && cd dist && vercel --yes",
    "deploy:staging": "EXPO_PUBLIC_APP_ENV=staging npx expo export -p web && cd dist && vercel --yes",
    "deploy:prod": "EXPO_PUBLIC_APP_ENV=production npx expo export -p web && cd dist && vercel --prod --yes",
    "android": "npx expo start --android",
    "ios": "npx expo start --ios",
    "web": "npx expo start --web",
    "server:dev": "NODE_ENV=development tsx server/index.ts",
    "db:push": "drizzle-kit push",
    "lint": "npx expo lint",
    "lint:fix": "npx expo lint --fix",
    "check:types": "tsc --noEmit",
    "check:format": "prettier --check \"**/*.{js,ts,tsx,css,json}\"",
    "format": "prettier --write \"**/*.{js,ts,tsx,css,json}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@expo/ngrok": "^4.1.3",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/native-stack": "^7.3.16",
    "@tanstack/react-query": "^5.90.7",
    "@vercel/node": "^5.1.8",
    "@vercel/postgres": "^0.10.0",
    "bcryptjs": "^2.4.3",
    "drizzle-orm": "^0.39.3",
    "drizzle-zod": "^0.7.0",
    "expo": "^54.0.31",
    "expo-blur": "^15.0.7",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.20",
    "expo-device": "~8.0.10",
    "expo-font": "~14.0.9",
    "expo-glass-effect": "~0.1.6",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-image-picker": "~17.0.10",
    "expo-linking": "~8.0.8",
    "expo-notifications": "~0.32.16",
    "expo-secure-store": "~15.0.5",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-updates": "~29.0.15",
    "expo-web-browser": "~15.0.10",
    "express": "^4.21.2",
    "http-proxy-middleware": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "react": "19.1.0",
