{
  "expo": {
    "name": "FastTrack v2.3",
    "slug": "fasttrack",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "fasttrack",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fasttrack.app",
      "entitlements": {
        "com.apple.developer.healthkit": true,
        "com.apple.developer.healthkit.background-delivery": true,
        "com.apple.security.application-groups": ["group.com.fasttrack.app"]
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSHealthShareUsageDescription": "FastTrack uses your health data to correlate fasting with weight, sleep, and activity patterns.",
        "NSHealthUpdateUsageDescription": "FastTrack can save your weight data to Apple Health."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#2DD4BF",
        "foregroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.fasttrack.app",
      "permissions": [
        "android.permission.health.READ_STEPS",
        "android.permission.health.READ_WEIGHT",
        "android.permission.health.WRITE_WEIGHT",
        "android.permission.health.READ_SLEEP",
        "android.permission.health.READ_HEART_RATE",
        "android.permission.health.READ_ACTIVE_CALORIES_BURNED",
        "android.permission.health.READ_HYDRATION",
        "android.permission.health.WRITE_HYDRATION"
      ]
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png",
      "name": "FastTrack v2.4",
      "shortName": "FastTrack",
      "description": "Track your fasting journey efficiently.",
      "themeColor": "#2DD4BF",
      "backgroundColor": "#ffffff",
      "display": "standalone",
      "startUrl": "/",
      "dir": "auto",
      "lang": "en",
      "preferRelatedApplications": false
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#0F172A"
          }
        }
      ],
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#2DD4BF",
          "sounds": []
        }
      ]
    ],
    "experiments": {
      "reactCompiler": true
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    }
  }
}