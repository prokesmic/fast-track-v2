{
  "expo": {
    "name": "Fasting app V2",
    "slug": "fasttrack",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "fasttrack",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fasttrack.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#2DD4BF",
        "foregroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.fasttrack.app"
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png",
      "name": "FastTrack",
      "shortName": "FastTrack",
      "description": "Track your fasting journey efficiently.",
      "themeColor": "#2DD4BF",
      "backgroundColor": "#ffffff",
      "display": "standalone",
      "startUrl": "/",
      "dir": "auto",
      "lang": "en",
      "preferRelatedApplications": false
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#0F172A"
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "reactCompiler": true
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    }
  }
}